<h3>Band Registration</h3>

<form class="registration" action="/user" method="post">
  <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>" />

  <p>
    <label for="band_name" class="form-label">Band Name</label>
    <input name="band_name" type="text">
  </p>

  <p>
    <label for="tour_info" class="form-label">Tour Info</label>
    <input name="tour_info" type="text">
  </p>

  <p>
    <input type="submit" value="Create Profile" class="form-button">
  </p>
</form>