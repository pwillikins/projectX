<div class="container">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h1>My Votes</h1>
    </div>
    <div class="panel-body">
      <table class="table table-striped" align="center">
        <th>Total Votes</th>
        <th>Song Name</th>
        <th>Artist Name</th>
        <% @user_votes.each do |array, count| %>
          <tr>
            <td><%= count %></td>
            <td><%= array.last.capitalize %></td>
            <td><%= array.first %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
